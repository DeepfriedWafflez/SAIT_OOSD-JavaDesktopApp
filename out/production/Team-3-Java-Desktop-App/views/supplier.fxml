<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.SupplierController">
   <children>
      <Button fx:id="btnHomeSupplier" layoutX="511.0" layoutY="45.0" mnemonicParsing="false" onAction="#btnHomeAction" text="Home">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <HBox layoutX="184.0" layoutY="234.0" spacing="20.0">
         <children>
            <Button fx:id="btnAddSupplier" mnemonicParsing="false" onAction="#btnAddSupplierAction" text="Add" />
            <Button fx:id="btnDeleteSupplier" mnemonicParsing="false" onAction="#btnDeleteSupplierAction" text="Delete" />
            <Pane />
            <Button fx:id="btnClearSupplier" mnemonicParsing="false" onAction="#btnClearSupplierAction" text="Clear" />
            <Button fx:id="btnEditSupplier" mnemonicParsing="false" onAction="#btnEditSupplierAction" text="Edit" />
            <Button fx:id="btnSaveSupplier" mnemonicParsing="false" onAction="#btnSaveSupplierAction" text="Save" />
         </children>
      </HBox>
      <TableView fx:id="tvSupplierList" layoutX="117.0" layoutY="288.0" prefHeight="262.0" prefWidth="482.0">
        <columns>
          <TableColumn fx:id="colSupplierId" prefWidth="117.0" text="Supplier ID" />
          <TableColumn fx:id="colSupplierName" minWidth="0.0" prefWidth="248.0" text="Supplier Name" />
        </columns>
      </TableView>
      <Pane layoutX="326.0" layoutY="120.0">
         <children>
            <TextField fx:id="tfSupplierName" layoutY="38.0" />
            <TextField fx:id="tfSupplierId" />
         </children>
      </Pane>
      <VBox layoutX="161.0" layoutY="121.0" prefHeight="61.0" prefWidth="134.0" spacing="25.0">
         <padding>
            <Insets left="5.0" />
         </padding>
         <children>
            <Label prefHeight="30.0" prefWidth="99.0" text="Supplier Id" />
            <Label prefHeight="30.0" prefWidth="99.0" text="Supplier Name" />
         </children>
      </VBox>
   </children>
</AnchorPane>
