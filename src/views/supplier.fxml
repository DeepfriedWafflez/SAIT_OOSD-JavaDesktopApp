<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.SupplierController">
   <children>
      <Button fx:id="btnHomeSupplier" layoutX="519.0" layoutY="14.0" mnemonicParsing="false" onAction="#btnHomeAction" text="Home">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnSelectSupplier" layoutX="374.0" layoutY="14.0" mnemonicParsing="false" onAction="#btnSelectSupplierAction" text="Select Supplier">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <TableView fx:id="tvSupplierList" layoutX="14.0" layoutY="53.0" prefHeight="333.0" prefWidth="335.0">
        <columns>
          <TableColumn fx:id="colSupplierId" prefWidth="117.0" text="Supplier ID" />
          <TableColumn fx:id="colSupplierName" minWidth="0.0" prefWidth="248.0" text="Supplier Name" />
        </columns>
      </TableView>
      <Pane layoutX="380.0" layoutY="259.0" />
      <Button fx:id="btnAddSupplier" layoutX="433.0" layoutY="289.0" mnemonicParsing="false" onAction="#btnAddSupplierAction" text="Add Supplier" />
      <Button fx:id="btnClearSupplier" layoutX="400.0" layoutY="329.0" mnemonicParsing="false" onAction="#btnClearSupplierAction" text="Clear" />
      <Button fx:id="btnUpdateSupplier" layoutX="524.0" layoutY="289.0" mnemonicParsing="false" onAction="#btnUpdateSupplierAction" text="Update Supplier" />
      <Label layoutX="349.0" layoutY="245.0" prefHeight="30.0" prefWidth="99.0" text="Supplier Name" />
      <TextField fx:id="tfSupplierName" layoutX="432.0" layoutY="247.0" />
      <Label layoutX="346.0" layoutY="198.0" prefHeight="30.0" prefWidth="99.0" text="Supplier Id" />
      <TextField fx:id="tfSupplierId" layoutX="433.0" layoutY="211.0" />
   </children>
</AnchorPane>
